<ivysettings>
	<settings defaultResolver="chain" />
	<resolvers>
		<chain name="chain">
			<ibiblio name="ibiblio" />
			<url name="cloudera" m2compatible="true">
				<artifact
					pattern="https://repository.cloudera.com/content/repositories/releases/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
			</url>
			<url name="repo1" m2compatible="true">
				<artifact
					pattern="http://repo1.maven.org/maven2/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
			</url>
		</chain>
	</resolvers>
	
</ivysettings>